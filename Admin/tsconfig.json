{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    //"allowJs": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": [
      "node_modules/@types/",
      "webpack-env"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot/dist",
    "wwwroot",
    "dist",
    "obj",
    "bin"

  ]
}